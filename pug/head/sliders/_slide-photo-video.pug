// Start slider section
.slider#top
	#preloader
			#status
	#bgndVideo.player(data-property="{videoURL:'https://youtu.be/kvJxX7fk0gI',containment:'.full-slider', showControls:false, autoPlay:true, loop:true, mute:true, startAt:0, opacity:1, quality:'default'}")
	.full-slider.background-image.intro(data-image='img/personal.jpg')
		// Start slide
		.slide.bg-mask.full-vh
			.container-slide.vertical-align.small-text.center
				.container
					.row
						.col-md-12
							.heading-title-big.letter-s-8 I'm photographer
							.description-slide I will do awesome photos for you !
	//Contol slider
	.dots-control-carousel#dots-control-full-slider
	// Strat Control carousel
	#control-full-slider.prev-next-block-rotate.opacity-control
		.wrap-prev
			.prev
				i(class="fa fa-angle-left" aria-hidden="true")
		.wrap-next
			.next
				i(class="fa fa-angle-right" aria-hidden="true")
	a(href="#into", class='smooth-scroll btn-down')
		i(class="fa fa-angle-down" aria-hidden="true")
// End slider section